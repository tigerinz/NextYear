<DrySQL>
  UPDATE ufsystem..UA_Account_Sub SET bClosing=0 WHERE cAcc_Id='{0}' AND iYear= {1} AND (bIsDelete=0 OR bIsDelete IS NULL) AND cSub_Id not like 'WA'

