    <Introduction>
      {newYearDB}..: new database name
      {oldYearDB}..: old database name
    </Introduction>
    <DrySQL>
      if (select count(*) from {newYearDB}..sysobjects where xtype='U' and name='fa_EvaluateMain')>0
      begin
      UPDATE {newYearDB}..fa_EvaluateMain SET iPeriod = 0
      end
      if (select count(*) from {newYearDB}..sysobjects where xtype='U' and name='fa_EvaluateVouchers')>0
      begin
      UPDATE {newYearDB}..fa_EvaluateVouchers SET iPeriod = 0
      end
      if (select count(*) from {newYearDB}..sysobjects where xtype='U' and name='fa_Cards')>0
      begin
      UPDATE {newYearDB}..fa_Cards SET sVoucherNum='',sEvaluateNum='',iInputPeriod = 0,iTransPeriod = 0,iDisposePeriod = 0,iOptType = 0
      end
      if (select count(*) from {newYearDB}..sysobjects where xtype='U' and name='fa_JKSet')>0
      begin
      UPDATE {newYearDB}..fa_JKSet SET dDataIn = NULL,dDataWriteIn = NULL
      end
    </DrySQL>
