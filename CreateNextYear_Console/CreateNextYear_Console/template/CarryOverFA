<SQLSentence id="DeleteInsertAccInformation">
    <Name>DeleteInsertAccInformation</Name>
  <ParamNames>{newYearDB},{oldYearDB}</ParamNames>
  <ParamNames>{newYearDB},{oldYearDB}</ParamNames>
    <Introduction>
      {newYearDB}..: new database name
      {oldYearDB}..: old database name
    </Introduction>
    <DrySQL>
      if (select count(*) from {newYearDB}..sysobjects where xtype='U' and name='AccInformation')>0
      begin
      if(select count(*) from {oldYearDB}..AccInformation)>0 DELETE FROM {newYearDB}..AccInformation WHERE lower([cSysID]) = 'fa';
      INSERT INTO {newYearDB}..AccInformation (cSysID,cID,cName,cCaption,cType,cValue,cDefault,bVisible,bEnable) select cSysID,cID,cName,cCaption,cType,cValue,cDefault,bVisible,bEnable from {oldYearDB}..AccInformation WHERE lower([cSysID]) = 'fa';
      end
    </DrySQL>
