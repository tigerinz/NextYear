  <SQLSentence id="DeleteInsertUA_HoldAuth">
    <Name>DeleteInsertUA_HoldAuth</Name>
    <ParamNames>{cAcc_Id},{newYear},{oldYear}</ParamNames>
 <Introduction>
   {0}..: acc Nu
    {newYear}..: new year
   {2}..: old year
 </Introduction>
 <DrySQL>
   DELETE FROM ufsystem..UA_HoldAuth WHERE cAcc_Id='{cAcc_Id}' AND iYear= {newYear};
   insert into ufsystem..UA_HoldAuth (cAcc_Id,iYear,cUser_Id,cAuth_Id) select cAcc_Id, {newYear},cUser_Id,cAuth_Id from ufsystem..UA_HoldAuth
   WHERE cAcc_Id='{cAcc_Id}' AND iYear={oldYear};
 </DrySQL>
</SQLSentence>
