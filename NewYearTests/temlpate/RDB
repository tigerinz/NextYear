      begin
      if(select count(*) from {newYearDB}..sysobjects where xtype='U' and name='GL_bdigest')>0 DELETE FROM {newYearDB}..GL_bdigest;
      if(SELECT COUNT(*) FROM {newYearDB}.sys.identity_columns AS cols INNER JOIN {newYearDB}.sys.sysobjects AS objects ON cols.object_id = objects.id WHERE (objects.name = 'GL_bdigest'))=1
      begin
