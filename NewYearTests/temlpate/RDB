  <SQLSentence id="DeleteInsertCode">
    <Name>DeleteInsertCode</Name>
    <ParamNames>{newYearDB},{oldYearDB}</ParamNames>
    <Introduction>
      {newYearDB}..: new database name
      {0}..: old database name
    </Introduction>
    <DrySQL>
      if (select count(*) from {oldYearDB}..Code)>0
      begin
      if(select count(*) from {newYearDB}..sysobjects where xtype='U' and name='Code')>0 DELETE FROM {newYearDB}..Code;
      if(SELECT COUNT(*) FROM {newYearDB}.sys.identity_columns AS cols INNER JOIN {newYearDB}.sys.sysobjects AS objects ON cols.object_id = objects.id WHERE (objects.name = 'Code'))=1
      begin
      SET IDENTITY_INSERT {newYearDB}..Code ON ;
      INSERT INTO {newYearDB}..Code (i_id,cclass,cclass_engl,cclassany,cclassany_engl,ccode,ccode_name,ccode_engl,igrade,bproperty,cbook_type,cbook_type_engl,chelp,cexch_name,cmeasure,bperson,bcus,bsup,bdept,bitem,cass_item,br,be,cgather,bend,bexchange,bcash,bbank,bused,bd_c,dbegin,dend,itrans,bclose,cother,iotherused,bcDefine1,bcDefine2,bcDefine3,bcDefine4,bcDefine5,bcDefine6,bcDefine7,bcDefine8,bcDefine9,bcDefine10,iViewItem,bGCJS,bCashItem)
      select i_id,cclass,cclass_engl,cclassany,cclassany_engl,ccode,ccode_name,ccode_engl,igrade,bproperty,cbook_type,cbook_type_engl,chelp,cexch_name,cmeasure,bperson,bcus,bsup,bdept,bitem,cass_item,br,be,cgather,bend,bexchange,bcash,bbank,bused,bd_c,dbegin,dend,itrans,bclose,cother,iotherused,bcDefine1,bcDefine2,bcDefine3,bcDefine4,bcDefine5,bcDefine6,bcDefine7,bcDefine8,bcDefine9,bcDefine10,iViewItem,bGCJS,bCashItem from {oldYearDB}..Code;
      SET IDENTITY_INSERT {newYearDB}..Code off;
      end
      else
      begin
      INSERT INTO {newYearDB}..Code (i_id,cclass,cclass_engl,cclassany,cclassany_engl,ccode,ccode_name,ccode_engl,igrade,bproperty,cbook_type,cbook_type_engl,chelp,cexch_name,cmeasure,bperson,bcus,bsup,bdept,bitem,cass_item,br,be,cgather,bend,bexchange,bcash,bbank,bused,bd_c,dbegin,dend,itrans,bclose,cother,iotherused,bcDefine1,bcDefine2,bcDefine3,bcDefine4,bcDefine5,bcDefine6,bcDefine7,bcDefine8,bcDefine9,bcDefine10,iViewItem,bGCJS,bCashItem)
