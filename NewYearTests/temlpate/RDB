    <ParamNames>{newYearDB},{oldYearDB}</ParamNames>
    <Introduction>
      {newYearDB}..: new database name
      {oldYearDB}..: old database name
    </Introduction>
    <DrySQL>
      if (select count(*) from {oldYearDB}..Ap_SStyleCode)>0
      begin
      if(select count(*) from {newYearDB}..sysobjects where xtype='U' and name='Ap_SStyleCode')>0 DELETE FROM {newYearDB}..Ap_SStyleCode;
      if(SELECT COUNT(*) FROM {newYearDB}.sys.identity_columns AS cols INNER JOIN {newYearDB}.sys.sysobjects AS objects ON cols.object_id = objects.id WHERE (objects.name = 'Ap_SStyleCode'))=1
      begin
      SET IDENTITY_INSERT {newYearDB}..Ap_SStyleCode ON ;
      INSERT INTO {newYearDB}..Ap_SStyleCode (Auto_ID,cSettleStyle,cexch_name,cCode,cFlag)
      select Auto_ID,cSettleStyle,cexch_name,cCode,cFlag from {oldYearDB}..Ap_SStyleCode;
      SET IDENTITY_INSERT {newYearDB}..Ap_SStyleCode off;
      end
      else
      begin
      INSERT INTO {newYearDB}..Ap_SStyleCode (Auto_ID,cSettleStyle,cexch_name,cCode,cFlag)
      select Auto_ID,cSettleStyle,cexch_name,cCode,cFlag from {oldYearDB}..Ap_SStyleCode;
      end
      end
    </DrySQL>
  </SQLSentence>
