<?xml version="1.0" encoding="utf-8" ?>
<SQLRepository>
  <SQLSentence id="restoreDB">
    <Name>restoreDB</Name>
    <ParamNames>{newYearDB},{ufmodelPath},{mdfPath},{ldfPath}</ParamNames>
    <Introduction>
      RESTORE DATABASE,
      {0}: database name
       {1}: path of bak file
      {2}: path of mdf
      {3}: path of ldf
    </Introduction>
    <DrySQL>
      RESTORE DATABASE {newYearDB}
      FROM  DISK = N'{ufmodelPath}'
      WITH  FILE = 1,  NOUNLOAD ,  STATS = 10,  RECOVERY ,  REPLACE ,
      MOVE N'UFModel' TO N'{mdfPath}',
      MOVE N'UFModel_log' TO N'{ldfPath}'
    </DrySQL>
  </SQLSentence>
  <SQLSentence id="AddMemo">
    <Name>AddMemo</Name>
    <ParamNames>{newYearDB}</ParamNames>
    <Introduction>AddMemo</Introduction>
    <DrySQL>
      if not exists (select count(*) from {newYearDB}..syscolumns where id = object_id(N'{newYearDB}..ProductStructures') and name = 'cMemo')
      alter table {newYearDB}..ProductStructures add cMemo varchar(200) null
    </DrySQL>
  </SQLSentence>
  <SQLSentence id="DeleteMCs">
    <Name>DeleteMCs</Name>
    <ParamNames>{newYearDB}</ParamNames>
    <Introduction>DeleteMCs</Introduction>
    <DrySQL>
      DELETE FROM {newYearDB}..MC_MyAccount;
      DELETE FROM {newYearDB}..MC_SendTime;
      DELETE FROM {newYearDB}..MC_MessageTemplateToList;
      DELETE FROM {newYearDB}..MC_MessageTemplateAttach;
      DELETE FROM {newYearDB}..MC_MessageTemplate;
    </DrySQL>
  </SQLSentence>
  <SQLSentence id="DeleteInsertFitem">
    <Name>DeleteInsertFitem</Name>
    <ParamNames>{newYearDB},{oldYearDB}</ParamNames>
    <Introduction>
      {newYearDB}..: new database name
      {0}..: old database name
    </Introduction>
    <DrySQL>
      if (select count(*) from {oldYearDB}..Fitem)>0
      begin
      if(select count(*) from  {newYearDB}..sysobjects where xtype='U' and name='Fitem')>0 DELETE FROM {newYearDB}..Fitem;
      if(SELECT COUNT(*) FROM  {newYearDB}.sys.identity_columns AS cols INNER JOIN  {newYearDB}.sys.sysobjects AS objects ON cols.object_id = objects.id WHERE (objects.name = 'Fitem'))=1
      begin
      SET IDENTITY_INSERT {newYearDB}..Fitem ON ;
      INSERT INTO {newYearDB}..Fitem (i_id,citem_class,citem_name,citem_text,crule,ctable,cClasstable)
      select i_id,citem_class,citem_name,citem_text,crule,ctable,cClasstable from {oldYearDB}..Fitem;
      SET IDENTITY_INSERT {newYearDB}..Fitem off;
      end
      else
      begin
      INSERT INTO {newYearDB}..Fitem (i_id,citem_class,citem_name,citem_text,crule,ctable,cClasstable)
      select i_id,citem_class,citem_name,citem_text,crule,ctable,cClasstable from {oldYearDB}..Fitem;
      end
      end
    </DrySQL>
  </SQLSentence>
  <SQLSentence id="DeleteInsertForeigncurrency">
    <Name>DeleteInsertForeigncurrency</Name>
    <ParamNames>{newYearDB},{oldYearDB}</ParamNames>
    <Introduction>
      {newYearDB}..: new database name
      {0}..: old database name
    </Introduction>
    <DrySQL>
