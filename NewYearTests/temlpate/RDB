      begin
      if(select count(*) from {newYearDB}..sysobjects where xtype='U' and name='Foreigncurrency')>0 DELETE FROM {newYearDB}..Foreigncurrency;
      if(SELECT COUNT(*) FROM {newYearDB}.sys.identity_columns AS cols INNER JOIN {newYearDB}.sys.sysobjects AS objects ON cols.object_id = objects.id WHERE (objects.name = 'Foreigncurrency'))=1
      begin
      SET IDENTITY_INSERT {newYearDB}..Foreigncurrency ON ;
      INSERT INTO {newYearDB}..Foreigncurrency (i_id,cexch_name,cexch_code,bcal,idec,nerror,iotherused)
      select i_id,cexch_name,cexch_code,bcal,idec,nerror,iotherused from {oldYearDB}..Foreigncurrency;
      SET IDENTITY_INSERT {newYearDB}..Foreigncurrency off;
      end
      else
