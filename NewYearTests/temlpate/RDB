      INSERT INTO {newYearDB}..KCHSItemPRN (CardNum,CardItemNum,CardItemName,CanNotSelect,ShowIt,LinkIt,LinkTbl,TableName,FieldName,FieldType,ReferType,ReferTable,CardSection,CardFormula1,CardFormula2,CanModify,MaxLength,MaxShowLen,NumPoint,IsNull,CanDelete,UserCheck,UserPrompt,FormatChar,IsMain,NeedSum,CalcField,LinkField,AliasNum,isSelfDef,COX,COY,tabidx)
      select CardNum,CardItemNum,CardItemName,CanNotSelect,ShowIt,LinkIt,LinkTbl,TableName,FieldName,FieldType,ReferType,ReferTable,CardSection,CardFormula1,CardFormula2,CanModify,MaxLength,MaxShowLen,NumPoint,IsNull,CanDelete,UserCheck,UserPrompt,FormatChar,IsMain,NeedSum,CalcField,LinkField,AliasNum,isSelfDef,COX,COY,tabidx from {oldYearDB}..KCHSItemPRN;
      end
      end
    </DrySQL>
  </SQLSentence>
  <SQLSentence id="DeleteInsertVoucherNumber">
    <Name>DeleteInsertVoucherNumber</Name>
    <ParamNames>{newYearDB},{oldYearDB}</ParamNames>
    <Introduction>
      {newYearDB}..: new database name
      {oldYearDB}..: old database name
    </Introduction>
    <DrySQL>
      if (select count(*) from {oldYearDB}..VoucherNumber)>0
      begin
      if(select count(*) from {newYearDB}..sysobjects where xtype='U' and name='VoucherNumber')>0 DELETE FROM {newYearDB}..VoucherNumber;
      if(SELECT COUNT(*) FROM {newYearDB}.sys.identity_columns AS cols INNER JOIN {newYearDB}.sys.sysobjects AS objects ON cols.object_id = objects.id WHERE (objects.name = 'VoucherNumber'))=1
      begin
      SET IDENTITY_INSERT {newYearDB}..VoucherNumber ON ;
      INSERT INTO {newYearDB}..VoucherNumber (AppName,CardNumber,CardName,iSize,dStartDate,bAllowHandWork,bRepeatReDo,bRdFlag,iStartNumber,iIsRdVoucher,Prefix1,Prefix1Len,Prefix1Rule,Prefix2,Prefix2Len,Prefix2Rule,Prefix3,Prefix3Len,Prefix3Rule,Glide,GlideLen,GlideRule)
      select AppName,CardNumber,CardName,iSize,dStartDate,bAllowHandWork,bRepeatReDo,bRdFlag,iStartNumber,iIsRdVoucher,Prefix1,Prefix1Len,Prefix1Rule,Prefix2,Prefix2Len,Prefix2Rule,Prefix3,Prefix3Len,Prefix3Rule,Glide,GlideLen,GlideRule from {oldYearDB}..VoucherNumber;
      SET IDENTITY_INSERT {newYearDB}..VoucherNumber off;
      end
      else
      begin
      INSERT INTO {newYearDB}..VoucherNumber (AppName,CardNumber,CardName,iSize,dStartDate,bAllowHandWork,bRepeatReDo,bRdFlag,iStartNumber,iIsRdVoucher,Prefix1,Prefix1Len,Prefix1Rule,Prefix2,Prefix2Len,Prefix2Rule,Prefix3,Prefix3Len,Prefix3Rule,Glide,GlideLen,GlideRule)
      select AppName,CardNumber,CardName,iSize,dStartDate,bAllowHandWork,bRepeatReDo,bRdFlag,iStartNumber,iIsRdVoucher,Prefix1,Prefix1Len,Prefix1Rule,Prefix2,Prefix2Len,Prefix2Rule,Prefix3,Prefix3Len,Prefix3Rule,Glide,GlideLen,GlideRule from {oldYearDB}..VoucherNumber;
      end
      end
    </DrySQL>
  </SQLSentence>
