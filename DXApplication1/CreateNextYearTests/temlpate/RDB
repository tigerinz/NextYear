  <SQLStatement id="UpdateUA_Period">
<Name>UpdateUA_Period</Name>
<Introduction>
  {0}..: acc no
  {1}..: new year
</Introduction>
<DrySQL>
  delete from UFSystem..UA_period WHERE cacc_id='{0}' and iyear={1};
  INSERT INTO UFSystem..UA_Period VALUES('{0}',{1},1, '{1}-01-01','{1}-01-31',0);
  INSERT INTO UFSystem..UA_Period VALUES('{0}',{1},2, '{1}-02-01','{1}-02-28',0);
  INSERT INTO UFSystem..UA_Period VALUES('{0}',{1},3, '{1}-03-01','{1}-03-31',0);
  INSERT INTO UFSystem..UA_Period VALUES('{0}',{1},4, '{1}-04-01','{1}-04-30',0);
  INSERT INTO UFSystem..UA_Period VALUES('{0}',{1},5, '{1}-05-01','{1}-05-31',0);
  INSERT INTO UFSystem..UA_Period VALUES('{0}',{1},6, '{1}-06-01','{1}-06-30',0);
  INSERT INTO UFSystem..UA_Period VALUES('{0}',{1},7, '{1}-07-01','{1}-07-31',0);
  INSERT INTO UFSystem..UA_Period VALUES('{0}',{1},8, '{1}-08-01','{1}-08-31',0);
  INSERT INTO UFSystem..UA_Period VALUES('{0}',{1},9, '{1}-09-01','{1}-09-30',0);
  INSERT INTO UFSystem..UA_Period VALUES('{0}',{1},10,'{1}-10-01','{1}-10-31',0);
  INSERT INTO UFSystem..UA_Period VALUES('{0}',{1},11,'{1}-11-01','{1}-11-30',0);
  INSERT INTO UFSystem..UA_Period VALUES('{0}',{1},12,'{1}-12-01','{1}-12-31',0)
</DrySQL>
 </SQLStatement>
  <SQLStatement id="DeleteInsertUA_HoldAuth">
 <Name>DeleteInsertUA_HoldAuth</Name>
 <Introduction>
   {0}..: acc Nu
   {1}..: new year
   {2}..: old year
 </Introduction>
 <DrySQL>
   DELETE FROM ufsystem..UA_HoldAuth WHERE cAcc_Id='{0}' AND iYear={1};
   insert into ufsystem..UA_HoldAuth (cAcc_Id,iYear,cUser_Id,cAuth_Id) select cAcc_Id,{1},cUser_Id,cAuth_Id from ufsystem..UA_HoldAuth
   WHERE cAcc_Id='{0}' AND iYear={2};
 </DrySQL>
</SQLStatement>
  <SQLStatement id="UpdataUA_Account_Sub">
<Name>UpdataUA_Account_Sub</Name>
<Introduction>
  {0}..: accNo
  {1}..: old year
</Introduction>
<DrySQL>
  UPDATE ufsystem..UA_Account_Sub SET bClosing=0 WHERE cAcc_Id='{0}' AND iYear={1} AND (bIsDelete=0 OR bIsDelete IS NULL) AND cSub_Id not like 'WA'

</DrySQL>
</SQLStatement>



