<?xml version="1.0" encoding="utf-8" ?>
<SQLTemplate>
  <SQLStatement id="restoreDB">
    <Name>restoreDB</Name>
    <Introduction>
      RESTORE DATABASE,
      {0}: database name
      {1}: path of bak file
      {2}: path of mdf
      {3}: path of ldf
    </Introduction>
    <DrySQL>
      RESTORE DATABASE {0}
      FROM  DISK = N'{1}'
      WITH  FILE = 1,  NOUNLOAD ,  STATS = 10,  RECOVERY ,  REPLACE ,
      MOVE N'UFModel' TO N'{2}',
      MOVE N'UFModel_log' TO N'{3}'
    </DrySQL>
  </SQLStatement>
  <SQLStatement id="AddMemo">
    <Name>AddMemo</Name>
    <Introduction>AddMemo</Introduction>
    <DrySQL>
      if not exists (select count(*) from {0}..syscolumns where id = object_id(N'{0}..ProductStructures') and name = 'cMemo') 
      alter table {0}..ProductStructures add cMemo varchar(200) null
    </DrySQL>
  </SQLStatement>
  <SQLStatement id="DeleteMCs">
    <Name>DeleteMCs</Name>
    <Introduction>DeleteMCs</Introduction>
    <DrySQL>
      DELETE FROM {0}..MC_MyAccount;
      DELETE FROM {0}..MC_SendTime;
      DELETE FROM {0}..MC_MessageTemplateToList;
      DELETE FROM {0}..MC_MessageTemplateAttach;
      DELETE FROM {0}..MC_MessageTemplate;
    </DrySQL>
  </SQLStatement>
  <SQLStatement id="DeleteInsertFitem">
    <Name>DeleteInsertFitem</Name>
    <Introduction>
      ufdata_006_2017..: new database name
      ufdata_006_2016..: old database name
    </Introduction>
    <DrySQL>
      if (select count(*) from ufdata_006_2016..Fitem)>0
      begin
      if(select count(*) from ufdata_006_2017..sysobjects where xtype='U' and name='Fitem')>0 DELETE FROM ufdata_006_2017..Fitem;
      if(SELECT COUNT(*) FROM ufdata_006_2017.sys.identity_columns AS cols INNER JOIN ufdata_006_2017.sys.sysobjects AS objects ON cols.object_id = objects.id WHERE (objects.name = 'Fitem'))=1
      begin
      SET IDENTITY_INSERT ufdata_006_2017..Fitem ON ;
      INSERT INTO ufdata_006_2017..Fitem (i_id,citem_class,citem_name,citem_text,crule,ctable,cClasstable)
      select i_id,citem_class,citem_name,citem_text,crule,ctable,cClasstable from ufdata_006_2016..Fitem;
      SET IDENTITY_INSERT ufdata_006_2017..Fitem off;
      end
      else
      begin
      INSERT INTO ufdata_006_2017..Fitem (i_id,citem_class,citem_name,citem_text,crule,ctable,cClasstable)
      select i_id,citem_class,citem_name,citem_text,crule,ctable,cClasstable from ufdata_006_2016..Fitem;
      end
      end
    </DrySQL>
  </SQLStatement>
