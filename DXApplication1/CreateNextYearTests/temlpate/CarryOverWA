  <SQLStatement id="Insert WA_account">
    <Name>Insert WA_account</Name>
    <Introduction>
      ufdata_006_2017..: new database name
      ufdata_006_2016..: old database name
    </Introduction>
    <DrySQL>
      if (select count(*) from ufdata_006_2016..WA_account)>0
      begin
      if(select count(*) from ufdata_006_2017..sysobjects where xtype='U' and name='WA_account')>0 DELETE FROM ufdata_006_2017..WA_account;
      if(SELECT COUNT(*) FROM ufdata_006_2017.sys.identity_columns AS cols INNER JOIN ufdata_006_2017.sys.sysobjects AS objects ON cols.object_id = objects.id WHERE (objects.name = 'WA_account'))=1
      begin
      SET IDENTITY_INSERT ufdata_006_2017..WA_account ON ;
      INSERT INTO ufdata_006_2017..WA_account (cGZGradeNum,cGZGradename,bHZGrade,bOnlyGZGrade,dStart_time,bOpenClose,cMoney,bBank,bMinus_tax,bInteger,iInteger_DW,iPsn_len,iLastMonth,iLastYear)
      select cGZGradeNum,cGZGradename,bHZGrade,bOnlyGZGrade,dStart_time,bOpenClose,cMoney,bBank,bMinus_tax,bInteger,iInteger_DW,iPsn_len,iLastMonth,iLastYear from ufdata_006_2016..WA_account;
      SET IDENTITY_INSERT ufdata_006_2017..WA_account off;
      end
      else
      begin
      INSERT INTO ufdata_006_2017..WA_account (cGZGradeNum,cGZGradename,bHZGrade,bOnlyGZGrade,dStart_time,bOpenClose,cMoney,bBank,bMinus_tax,bInteger,iInteger_DW,iPsn_len,iLastMonth,iLastYear)
      select cGZGradeNum,cGZGradename,bHZGrade,bOnlyGZGrade,dStart_time,bOpenClose,cMoney,bBank,bMinus_tax,bInteger,iInteger_DW,iPsn_len,iLastMonth,iLastYear from ufdata_006_2016..WA_account;
      end
      end
    </DrySQL>
  </SQLStatement>
